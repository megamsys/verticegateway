#!/bin/sh

if [ -d "/usr/share/megam/megamgateway" ]; then
  rm -r /usr/share/megam/megamgateway
fi

if [ -d "/var/lib/megam/megamgateway" ]; then
  rm -r /var/lib/megam/megamgateway
fi

if [ -d "/var/log/megam/megamgateway" ]; then
  rm -r /var/log/megam/megamgateway
fi

if [ -d "/var/run/megam/megamgateway" ]; then
  rm -r /var/run/megam/megamgateway
fi
